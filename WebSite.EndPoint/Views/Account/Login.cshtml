        <ul>
            <li>
                <button id="darkButton" class="btn btn-sm dark-buttton">Dark</button>
            </li>
            <li>
                <button class="btn btn-sm rtl-button">RTL</button>
            </li>
            <li class="color-picker">
                <input type="color" class="form-control form-control-color" id="ColorPicker1" value="#e22454"
                    title="Choose your color">
            </li>
        </ul>
    </div>*@
    <!-- theme Setting End -->

    <div class="bg-overlay"></div>

    <!-- latest jquery-->
    <script src="~/theme/assets/js/jquery-3.5.1.min.js"></script>

    <!-- Bootstrap js-->
    <script src="~/theme/assets/js/bootstrap/bootstrap.bundle.min.js"></script>

    <!-- feather icon js-->
    <script src="~/theme/assets/js/feather/feather.min.js"></script>

    <!-- lazyload js-->
    <script src="~/theme/assets/js/lazysizes.min.js"></script>

    <!-- Slick js-->
    <script src="~/theme/assets/js/slick/slick.js"></script>
    <script src="~/theme/assets/js/slick/slick-animation.min.js"></script>
    <script src="~/theme/assets/js/slick/custom_slick.js"></script>

    <!-- Notify js-->
    <script src="~/theme/assets/js/bootstrap/bootstrap-notify.min.js"></script>

    <!-- script js -->
    <script src="~/theme/assets/js/theme-setting.js"></script>
    <script src="~/theme/assets/js/script.js"></script>
    <script src="~/theme/assets/js/home-script.js"></script>

</body>



